<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".find_password">

    <android.widget.Button
        android:id="@+id/find_password_button1"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="254dp"
        android:layout_marginTop="233dp"
        android:background="@drawable/find_password_button1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <android.widget.Button
        android:id="@+id/find_password_button2"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="254dp"
        android:layout_marginTop="290dp"
        android:background="@drawable/find_password_button2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <android.widget.Button
        android:id="@+id/find_password_button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="368dp"
        android:background="@drawable/find_password_button3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription,ImageContrastCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/find_password_input1"
        android:layout_width="240dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="240dp"
        android:background="@null"
        android:ems="10"
        android:hint="이메일을 입력해주세요"
        android:inputType="textPassword"
        android:textColorHint="#C1C1C1"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="Autofill,HardcodedText,SpUsage,SpeakableTextPresentCheck,TextContrastCheck,TextSizeCheck,TouchTargetSizeCheck,VisualLintOverlap" />

    <EditText
        android:id="@+id/find_password_input2"
        android:layout_width="240dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="295dp"
        android:background="@null"
        android:ems="10"
        android:hint="인증번호를 입력해주세요"
        android:inputType="textPassword"
        android:textColorHint="#C1C1C1"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="Autofill,HardcodedText,SpUsage,SpeakableTextPresentCheck,TextContrastCheck,TextSizeCheck,TouchTargetSizeCheck,VisualLintOverlap" />

    <ImageButton
        android:id="@+id/find_password_common_backbutton"
        android:layout_width="33dp"
        android:layout_height="41dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="27dp"
        android:background="@android:color/transparent"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/common_backbutton"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/find_password_input1_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="240dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/find_password_input1"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <ImageView
        android:id="@+id/find_password_input2_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="298dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/find_password_input2"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:text="비밀번호 찾기"
        android:textAlignment="center"
        android:textColor="@color/text_75A3E9_FFFFFF"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>