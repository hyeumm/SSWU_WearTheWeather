<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background_light"
    tools:context=".recommended_music">

    <ImageView
        android:id="@+id/recommended_music_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:layout_marginStart="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.291"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/recommended_music_1"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <RelativeLayout
        android:layout_width="240dp"
        android:layout_height="489dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="27dp">

        <WebView
            android:id="@+id/webview_1"
            android:layout_width="240dp"
            android:layout_height="135dp" />

        <WebView
            android:id="@+id/webview_2"
            android:layout_marginTop="177dp"
            android:layout_width="240dp"
            android:layout_height="135dp" />

        <WebView
            android:id="@+id/webview_3"
            android:layout_width="240dp"
            android:layout_height="135dp"
            android:layout_marginTop="354dp" />

    </RelativeLayout>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_marginTop="75dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="360dp"
            android:layout_height="60dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/common_menubar"
            tools:ignore="ContentDescription,ImageContrastCheck" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginTop="13dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/common_menubar_button1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageButton6"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck,ImageContrastCheck" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginTop="13dp"
            android:layout_marginLeft="90dp"
            android:background="@drawable/common_menubar_button2_blue"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageButton7"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck,DuplicateClickableBoundsCheck,ImageContrastCheck" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="13dp"
            android:background="@drawable/common_menubar_button3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView8"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck,ImageContrastCheck" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginTop="13dp"
            android:layout_marginLeft="230dp"
            android:background="@drawable/common_menubar_button4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageButton7"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="13dp"
            android:background="@drawable/common_menubar_button5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageButton8"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck,ImageContrastCheck" />


    </FrameLayout>


</LinearLayout>






